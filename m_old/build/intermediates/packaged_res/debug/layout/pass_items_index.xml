<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/c_e8">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_view"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/od_it_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/c_w"
        android:drawablePadding="5dp"
        android:drawableTop="@drawable/tuoguanguoqi"
        android:gravity="center"
        android:maxEms="4"
        android:padding="10dp"
        android:text="@string/Escrow_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/js_overdue_layout"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/js_overdue_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="0.8dp"
        android:layout_marginRight="0.8dp"
        android:background="@color/c_w"
        android:drawablePadding="5dp"
        android:drawableTop="@drawable/jishen"
        android:gravity="center"
        android:maxEms="4"
        android:padding="10dp"
        android:text="@string/Season_trial_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toBottomOf="@+id/od_it_layout"
        app:layout_constraintLeft_toRightOf="@+id/od_it_layout"
        app:layout_constraintRight_toLeftOf="@+id/ns_overdue_layout"
        app:layout_constraintTop_toTopOf="@+id/od_it_layout" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/ns_overdue_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/c_w"
        android:drawablePadding="5dp"
        android:drawableTop="@drawable/nianshen"
        android:gravity="center"
        android:maxEms="4"
        android:padding="10dp"
        android:text="@string/The_annual_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toBottomOf="@+id/od_it_layout"
        app:layout_constraintLeft_toRightOf="@+id/js_overdue_layout"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/od_it_layout" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/np_over_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="0.8dp"
        android:background="@color/c_w"
        android:drawablePadding="10dp"
        android:drawableTop="@drawable/nianpiao"
        android:gravity="center"
        android:maxEms="4"
        android:padding="10dp"
        android:text="@string/The_ticket_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/od_it_layout"
        app:layout_constraintTop_toBottomOf="@+id/od_it_layout" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/bx_overdue_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/c_w"
        android:drawablePadding="10dp"
        android:drawableTop="@drawable/baoxian1"
        android:gravity="center"
        android:maxEms="4"
        android:padding="10dp"
        android:text="@string/Insurance_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toBottomOf="@+id/np_over_layout"
        app:layout_constraintLeft_toLeftOf="@+id/js_overdue_layout"
        app:layout_constraintRight_toRightOf="@+id/js_overdue_layout"
        app:layout_constraintTop_toTopOf="@+id/np_over_layout" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/hb_overdue_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/c_w"
        android:drawablePadding="10dp"
        android:drawableTop="@drawable/huanbao"
        android:gravity="center"
        android:maxEms="4"
        android:padding="10dp"
        android:text="@string/Green_flag_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toBottomOf="@+id/np_over_layout"
        app:layout_constraintLeft_toLeftOf="@+id/ns_overdue_layout"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/np_over_layout" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/pass_construction_waste_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="0.8dp"
        android:background="@color/c_w"
        android:drawablePadding="5dp"
        android:drawableTop="@drawable/cwa"
        android:gravity="center"
        android:maxLines="4"
        android:padding="10dp"
        android:text="@string/pass_construction_waste"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/od_it_layout"
        app:layout_constraintTop_toBottomOf="@+id/np_over_layout" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/dlns_overdue_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/c_w"
        android:drawablePadding="5dp"
        android:drawableTop="@drawable/yunshu"
        android:gravity="center"
        android:padding="10dp"
        android:text="@string/The_annual_road_transport_permit_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toBottomOf="@+id/pass_construction_waste_layout"
        app:layout_constraintLeft_toLeftOf="@+id/bx_overdue_layout"
        app:layout_constraintRight_toRightOf="@+id/bx_overdue_layout"
        app:layout_constraintTop_toTopOf="@+id/pass_construction_waste_layout" />

    <com.cangmaomao.lib.view.CustomTextView
        android:id="@+id/jsz_overdue_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/c_w"
        android:drawablePadding="5dp"
        android:drawableTop="@drawable/jiashi"
        android:gravity="center"
        android:padding="10dp"
        android:text="@string/Driver_license_expired"
        android:textColor="@color/c_2c"
        android:textSize="@dimen/text_size"
        app:layout_constraintBottom_toBottomOf="@+id/pass_construction_waste_layout"
        app:layout_constraintLeft_toLeftOf="@+id/ns_overdue_layout"
        app:layout_constraintRight_toRightOf="@+id/ns_overdue_layout"
        app:layout_constraintTop_toTopOf="@+id/pass_construction_waste_layout" />
</android.support.constraint.ConstraintLayout>
